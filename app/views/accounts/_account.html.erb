<div id="<%= dom_id @account %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Username:</strong>
    <%= @account.username %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Image data:</strong>
    <%= image_tag @account.image_url if @account.image %>
  </p>


</div>
